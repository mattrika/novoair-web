{
  "name": "ng-starter-no-semi-four-space",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve",
    "build": "ng build",
    "build:alternative": "NODE_ENV=production node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npx biome lint ./src --apply",
    "prepare": "husky install .client/.husky",
    "format:ts": "npx biome format ./src --write",
    "format:html": "prettier --write 'src/**/*.{html,xml,scss,css,less}'",
    "format": "npm run format:ts && npm run format:html",
    "ng-update": "ng update @angular/core @angular/cli @angular/cdk @angular/material @schematics/angular --allow-dirty",
    "make-env-file": "tsc src/environments/make-env-file.ts && node src/environments/make-env-file.js && rm src/environments/make-env-file.js"
  },
  "dependencies": {
    "@angular/animations": "^16.2.4",
    "@angular/cdk": "^16.2.3",
    "@angular/common": "^16.2.4",
    "@angular/compiler": "^16.2.4",
    "@angular/core": "^16.2.4",
    "@angular/forms": "^16.2.4",
    "@angular/material": "^16.2.3",
    "@angular/platform-browser": "^16.2.4",
    "@angular/platform-browser-dynamic": "^16.2.4",
    "@angular/router": "^16.2.4",
    "@ng-web-apis/common": "^3.0.2",
    "@ngneat/hot-toast": "^5.0.2",
    "@ngneat/overview": "^5.1.1",
    "daisyui": "^3.7.3",
    "radash": "^11.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "^16.2.1",
    "@angular/compiler-cli": "^16.2.4",
    "@biomejs/biome": "1.1.2",
    "@biomejs/cli-linux-x64": "1.1.2",
    "@schematics/angular": "^16.2.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jasmine": "~4.3.5",
    "@types/node": "^20.5.9",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "pretty-quick": "^3.1.3",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.3",
    "tailwindcss-elevation": "^2.0.0",
    "typescript": "~5.1.6"
  },
  "lint-staged": {
    "*.{html,xml,scss,css,less}": "npx prettier --write",
    "*.{ts,js}": [
      "npx biome format --write",
      "npx biome lint --apply"
    ]
  }
}
