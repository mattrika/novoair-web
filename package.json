{
  "name": "ng-starter-no-semi-four-space",
  "version": "11.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "precommit": "lint-staged",
    "format:ts": "prettier --write 'src/**/*.{js,ts,scss,css,less,jsx,tsx,json}'",
    "format:html": "prettier --write 'src/**/*.{html,xml}'",
    "format:specs": "prettier --write 'e2e/**/*.*'",
    "format": "npm run format:ts && npm run format:html && npm run format:specs",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.3",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/forms": "~11.0.3",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/router": "~11.0.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.3",
    "@angular/cli": "~11.0.3",
    "@angular/compiler-cli": "~11.0.3",
    "@angular/language-service": "~11.0.3",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.10",
    "codelyzer": "^6.0.1",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "9.1.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~4.0.5"
  },
  "lint-staged": {
    "*.ts": "tslint --fix",
    "*.{js,ts,scss,css,less,jsx,tsx,json}": "prettier --write",
    "*.{html,xml}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": ""
    }
  }
}
