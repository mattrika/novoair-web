{
  "name": "ng-starter-no-semi-four-space",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:use-env": "npm run make-env-file && ng build",
    "build:alternative": "NODE_ENV=production node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "tsc --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "lint:debug": "TIMING=1 ng lint --fix",
    "register-git-hooks": "npx simple-git-hooks",
    "format:ts": "prettier --write 'src/**/*.{js,ts,scss,css,less,jsx,tsx,json}'",
    "format:html": "prettier --write 'src/**/*.{html,xml}'",
    "format": "npm run format:ts && npm run format:html",
    "ng-update": "ng update @angular/core @angular/cli @angular-eslint/schematics @schematics/angular --allow-dirty",
    "make-env-file": "tsc make-env-file.ts && node make-env-file.js && rm make-env-file.js",
    "analyze:build": "ng build --stats-json",
    "analyze:view": "webpack-bundle-analyzer ./dist/ng-starter-no-semi-four-space/stats.json",
    "analyze": "npm run analyze:build && npm run analyze:view"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/cdk": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/forms": "^14.2.1",
    "@angular/material": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "@ng-web-apis/common": "^2.0.1",
    "@ngneat/hot-toast": "^4.1.0",
    "@ngneat/overview": "^3.0.4",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular-eslint/builder": "14.0.4",
    "@angular-eslint/eslint-plugin": "14.0.4",
    "@angular-eslint/eslint-plugin-template": "14.0.4",
    "@angular-eslint/schematics": "14.0.4",
    "@angular-eslint/template-parser": "14.0.4",
    "@angular/cli": "^14.2.2",
    "@angular/compiler-cli": "^14.2.1",
    "@schematics/angular": "^14.2.2",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "simple-git-hooks": "^2.8.0",
    "tailwind-scrollbar": "^2.0.1",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.8.3",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "lint-staged": {
    "*.{scss,css,less,json}": "prettier --write",
    "*.{html,xml}": "prettier --write",
    "*.{ts,js}": "eslint --cache --fix"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}